{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off",
        "listitem": "off"
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id": "off",
        "marquee": "off"
      }
    ],
    "no-inline-styles": "off",
    "meta-viewport": "off",
    "meta-charset-utf-8": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "user-select",
          "overflow-anchor",
          "scrollbar-width"
        ]
      }
    ]
  }
}